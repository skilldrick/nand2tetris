/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not (in=a, out=notA);
    Not (in=b, out=notB);
    And (a=a, b=notB, out=aAndNotB);
    And (a=notA, b=b, out=bAndNotA);
    Or  (a=aAndNotB, b=bAndNotA, out=out);
}
